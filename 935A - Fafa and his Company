import java.util.*;
public class A_Fafa_and_his_Company {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int n=sc.nextInt();
        int c=1;
        int ans=0;
        for(int i=2;i<n;i++){
            ans=n-i;
            if(ans%i==0){
                c++;
            }
        }
        System.out.println(c);
}
}
