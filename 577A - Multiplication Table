import java.util.*;
public class A_Multiplication_Table {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        long n=sc.nextLong();
        long x=sc.nextLong();
        long c=0;
        
            for (int i = 1; i <= n; i++) {
                if (x % i == 0 && x / i <= n) {
                    c++;
                }
        }
        System.out.println(c);
    
}
}
