import java.util.*;
public class A_Only_Pluses {
    public static void main(String[] args) {
        Scanner sc=new Scanner(System.in);
        int t=sc.nextInt();
        while(t-- >0){
        int arr[]=new int[3];
        for(int i=0;i<3;i++){
            arr[i]=sc.nextInt();
        }
        int n=5;
        while(n>0){
            Arrays.sort(arr);
            arr[0]+=1;
            n--;
        }
        System.out.println(arr[0]*arr[1]*arr[2]);

        }
}
}
